<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="rescue.laundry_sequence_number" model="ir.sequence">
            <field name="name">Laundry Line Sequence</field>
            <field name="code">laundry.line.sequence</field>
            <field name="padding">6</field>
            <field name="number_next_actual">100000</field>
        </record>
    </data>
    <data noupdate="1">
        <record id="rescue.laundry_washings_sequence_number" model="ir.sequence">
            <field name="name">Washings sequence</field>
            <field name="code">washings.sequence</field>
            <field name="padding">6 </field>
            <field name="prefix">GT/</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="company_id" eval="False"/>
        </record>
        <record id="rescue.laundry_order_sequence_number" model="ir.sequence">
            <field name="name">Sale Order Washings sequence</field>
            <field name="code">sale.order.washings.sequence</field>
            <field name="padding">6 </field>
            <field name="prefix">L/</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    
</odoo>
